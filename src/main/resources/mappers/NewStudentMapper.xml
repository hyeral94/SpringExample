<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.hyeral.ex.lesson04.dao.NewStudentDAO">
 	<insert id="insertStudent" parameterType="com.hyeral.ex.lesson04.model.NewStudent"
 	useGeneratedKeys="true" keyProperty="id">
 		INSERT INTO `new_student`
 		(
 			`name`
 			, `phoneNumber`
 			, `email`
 			, `dreamJob`
 			, `createdAt`
 			, `updatedAt`
 		)
 		VALUES
 		(
 			#{name}
 			, #{phoneNumber}
 			, #{email}
 			, #{dreamJob}
 			, now()
 			, now()
 		)
 	</insert>
 
 
 </mapper>